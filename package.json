{
  "name": "@jupiterone/graph-crowdstrike",
  "version": "2.1.0",
  "description": "A template for JupiterOne graph converters.",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "repository": "https://github.com/JupiterOne/graph-crowdstrike",
  "license": "MPL-2.0",
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "j1-integration collect --disable-schema-validation",
    "graph": "j1-integration visualize",
    "graph:types": "j1-integration visualize-types",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "format:check": "prettier --check '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc",
    "test": "jest",
    "test:env": "LOAD_ENV=1 yarn test",
    "test:ci": "yarn format:check && yarn lint && yarn type-check && yarn test",
    "build": "tsc -p tsconfig.dist.json --declaration && cp README.md dist/README.md",
    "prepush": "yarn format:check && yarn lint && yarn type-check && jest --changedSince main"
  },
  "dependencies": {
    "@jupiterone/integration-sdk-runtime": "^8.2.1",
    "node-fetch": "^2.6.0"
  },
  "peerDependencies": {
    "@jupiterone/integration-sdk-core": "^8.2.1"
  },
  "devDependencies": {
    "@jupiterone/integration-sdk-dev-tools": "^8.2.1",
    "@jupiterone/integration-sdk-testing": "^8.2.1"
  }
}
